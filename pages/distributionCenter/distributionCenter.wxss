.page{
    background: #f8f8f8;
    color: #333;
    font-family: Microsoft YaHei;
    font-size: 14px;
}
.userinfo{
    background:url("http://ws7.wshoto.com/attachment/images/2/2017/08/EQAaGKqkXuE3xLl1dygKpFHXDaOA1U.png") no-repeat center center;
    background-size:100% 100%;
    height: 400rpx;
}
.userinfo-tp{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-top: 68rpx;
}
.userphoto{
    width: 128rpx;
    height: 128rpx;
    border-radius: 50%;
    overflow: hidden;
    margin-left: 28rpx;
}
.userphoto image{
    display: block;
    width: 100%;
    height: 100%;
}
.userinfo{
    position:relative;
}
.userinfo-item{
    margin-left: 30rpx;
}
.userinfo-lf{
    display: flex;
    justify-content: center space-around;
    align-items:center;
}
.userinfo-p{
    display: block;
    font-size: 12px;
    color:#fff;
    line-height: 20px;
}
.userinfo-bt{
    width: 100%;
    margin-top: 50rpx;
    border-top:1px solid rgba(255,255,255,.1);    
    position: absolute;
    bottom:40rpx;
    left:0px;
}
.userinfo-cost{
    padding-top: 30rpx;
    font-size: 14px;
    color:#fff;
    text-align: center;
}
.userinfo-lr.deltriangle{
    width:20rpx;
    height:20rpx;    
    border-top:1px solid #fff;
    border-left:1px solid #fff;
    margin-right: 52rpx;
}
.mypartner{
    height: 100rpx;
    line-height: 100rpx;
    padding-left: 20rpx;
    border-bottom: 1px solid #F6F5FA;
}
.mypartner image{
    width:40rpx;
    height:40rpx;
    position: relative;
    top:4rpx;
    left:0px;
}
.mypartner span{
   font-size: 15px;
   color:#333;
   margin-left: 30rpx;
}
.partner-list{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.partner-content{
    padding:50rpx 0rpx;
    text-align: center;
    flex: 1;
}
.partner-item{
    text-align: center;
    border-right:1px solid #eee;    
}
.last-item{
    border:none;
}
.partner-tp{
    font-size:14px;
    color: #999;
    line-height: 14px;
}
.partner-md{
    display: block;
    width: 40rpx;
    height:40rpx;
    margin:0 auto;
    margin-top:30rpx;
    line-height: 14px;
   
}
.partner-bt{
    font-size: 14px;
    color:#EE0B09;
    line-height: 14px;
    margin-top: 30rpx;
}
.goWithdrawals{
    height:100rpx;
    line-height:100rpx;    
    border-bottom:1px solid #F5F5F5;
    display: flex;
    justify-content:space-between;
    align-items: center;
    /* margin-bottom: 50rpx; */
    padding-left:20rpx;    
}
.goWithdrawals-lf,.goWithdrawals-lr{
    display: flex;
    justify-content:space-between;
    align-items: center;
}
.goWithdrawals-lf text{
    font-size:15px;
    color:#333;
    margin-left:30rpx;
}
.goWithdrawals-lr text{
    font-size:14px;
    color:#333;
    margin-right:30rpx;
}
/* .goWithdrawals-lr text:nth-child(1){
    margin-right:10rpx;
} */
.goWithdrawals-img{
    margin:0rpx; 
}