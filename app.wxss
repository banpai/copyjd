/* 新增 */
.clear {clear: both;height: 0px;line-height: 0px;font-size: 0px;overflow: hidden;visibility: hidden; }
.clearfix:after {content: "";display: block;height: 0;clear: both;visibility: hidden; }
.fl{float: left;}
.fr{float: right;}
.lr1{overflow: hidden;text-overflow: ellipsis;-ms-text-overflow:ellipsis;white-space:nowrap;}
.lr2{overflow: hidden;text-overflow: ellipsis;display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 2;}
.lr3{overflow: hidden;text-overflow: ellipsis;display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 3;}

/* loadding */
.loadding{
    width:36rpx;
    height: 36rpx;
    margin: 0 auto;
    margin-top: 180rpx;
}
.loadding image{
    display: block;
    width: 100%;
    height: 100%;
}

/* empty */
.empty{
    padding-top: 100rpx;
    background: #fff;
  }
  .empty text{
    display: block;
    width: 100%;
    text-align: center;
  }
  .empty .empty-icon{
    width: 260rpx;
    height: 260rpx;
    background: #DFE0E8;
    border-radius: 100%;
    position: relative;
    margin: 0 auto;
    margin-bottom: 50rpx;
  }
  .empty .empty-icon image{
    width: 150rpx;
    height: 150rpx;
    left: 50%;
    top: 50%;
    position: relative;
    transform: translate(-50%,-50%);
  }
  /* 分割块 */
  .delblock{
    width:100%;
    height:20rpx;
    background:#F8F8F8;    
  }
  .deltriangle{
    width: 18rpx;
    height: 18rpx;
    border-top:1px solid #888; 
    border-left:1px solid #888; 
    transform: rotate(135deg);
  }